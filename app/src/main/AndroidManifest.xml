<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nullgr.androidcore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".location.RxLocationManagerExampleActivity" />

        <activity android:name=".intents.CommonIntentsExampleActivity" />

        <!-- declare this activity in your manifest file to use CommonIntentsExtensions#launchForResult
        or RxLocationManager-->
        <activity android:name="com.nullgr.corelibrary.rx.rxresult.RxResolveResultActivity" />

        <activity android:name=".fonts.FontsAndSpansExampleActivity" />

        <activity android:name=".date.DateUtilsExampleActivity" />

        <activity android:name=".resources.ResourcesExampleActivity" />

        <activity android:name=".adapter.AdapterExampleActivity" />
    </application>

</manifest>